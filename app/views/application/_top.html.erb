<!-- Header Start -->
<header id="header">
  <!-- Header Top Bar Start -->
  <div class="top-bar">
    <div class="slidedown collapse">
      <div class="container">
        <div class="phone-email pull-right">
          <i class="icon-phone"></i> 电话 : <%= public_config.tel %>&nbsp;
          <i class="icon-envelope"></i> Email : <%= public_config.mail %>&nbsp;
          <i class="icon-comments"></i>QQ: <%= public_config.qq %>
        </div>

      </div>
    </div>
  </div>
  <!-- Header Top Bar End -->
  <!-- Main Header Start -->
  <div class="main-header">
    <div class="container">
      <!-- TopNav Start -->
      <div class="topnav navbar-header">
        <a class="navbar-toggle down-button" data-toggle="collapse" data-target=".slidedown">
          <i class="icon-angle-down icon-current"></i>
        </a>
      </div>
      <!-- TopNav End -->
      <!-- Logo Start -->
      <div class="logo pull-left">
        <h1>


          <%= image_tag public_config.logo.url,height:60 %>

        </h1>
      </div>
      <!-- Logo End -->
      <!-- Mobile Menu Start -->
      <div class="mobile navbar-header">
        <a class="navbar-toggle" data-toggle="collapse" href=".html">
          <i class="icon-reorder icon-2x"></i>
        </a>
      </div>
      <!-- Mobile Menu End -->
      <!-- Menu Start -->
      <nav class="collapse navbar-collapse menu">
        <ul class="nav navbar-nav sf-menu">
          <li>


            <%= link_to '关于我们','/','data-no-turbolink' => true %>

          </li>

          <li>
            <a href=<%= news_index_path %> class="sf-with-ul">
              新闻中心

            </a>

          </li>

          <li>
            <a href=<%= wenhuas_path %> class="sf-with-ul">
              企业文化

            </a>

          </li>



          <li>
            <a href=<%= products_path %> class="sf-with-ul">
              产品中心

            </a>

          </li>
          <li>
            <a href=<%= contacts_path %> class="sf-with-ul">
              联系我们

            </a>

          </li>

        </ul>
      </nav>
      <!-- Menu End -->
    </div>
  </div>
  <!-- Main Header End -->
</header>
<!-- Header End -->